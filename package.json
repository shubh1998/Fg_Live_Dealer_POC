{
  "name": "user-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mui/material": "5.2.4",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "axios": "0.24.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-router-dom": "6.1.1",
    "react-scripts": "5.0.0",
    "redux-saga": "1.1.3",
    "web-vitals": "2.1.2",
    "eslint": "6.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "standard": {
    "parser": "@babel/eslint-parser",
    "env": {
      "jest": true,
      "browser": true
    },
    "globals": [
      "artifacts",
      "tronWeb",
      "contract",
      "before",
      "assert",
      "after"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.5",
    "@babel/eslint-parser": "7.16.5",
    "@babel/preset-react": "7.16.5",
    "eslint": "7.12.1",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "standard": "16.0.4"
  }
}
